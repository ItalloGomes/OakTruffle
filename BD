create database login;                           
                      
use login;                      
                                             
create table cadastros (                                             
                          idUsuario int primary key auto_increment,                       
                          Nome varchar(50),   
                          Email varchar(50),
                          Senha varchar(12)                   
                       );                      
                                             
create table dados_recebidos (                                             
                               Temperatura varchar (10),              
                               Umidade varchar (10)
                              ); 
